OPENAI_API_KEY=your_openai_api_key
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Optional: Web server
PORT=8000
HOST=0.0.0.0

# Optional: Database
DB_PASSWORD=change_me
DATABASE_URL=postgresql://salesdesk:${DB_PASSWORD}@localhost:5432/salesdesk
# For Docker Compose, use the service hostname instead of localhost:
# DATABASE_URL=postgresql://salesdesk:${DB_PASSWORD}@postgres:5432/salesdesk

# Optional: Redis
REDIS_URL=redis://localhost:6379/0
# For Docker Compose, use the service hostname instead of localhost:
# REDIS_URL=redis://redis:6379/0
PERSISTENCE_BACKEND=memory  # memory|redis|postgres
WEBHOOK_SHARED_SECRET=devsecret
VERIFY_OIDC=false
PUBSUB_AUDIENCE=your-pubsub-audience
OIDC_SKIP_SIGNATURE=true

# Slack notifications (optional)
SLACK_WEBHOOK_URL=
